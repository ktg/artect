	<owl:Class rdf:ID="ARMarker">
		<rdfs:label>AR Marker</rdfs:label>
		<rdfs:subClassOf>
			<owl:Class rdf:ID="PhysicalThing" />
		</rdfs:subClassOf>
	</owl:Class>
	<ect:ThingDetails rdf:ID="WebcamDetails">
		<ect:thingType rdf:resource="#Webcam" />	
		<ect:createLink>
			<ect:Link rdf:ID="sourceSinkARMarkerLink">
				<rdfs:label>source->sink</rdfs:label>
				<ect:source>source</ect:source>
				<ect:destination>sink</ect:destination>
				<ect:reverseLink>true</ect:reverseLink>
			</ect:Link>	
		</ect:createLink>
	</ect:ThingDetails>
	<ect:ARMarker rdf:ID="ARMarkerA">
		<rdfs:label>AR Marker A</rdfs:label>
		<ect:classification>AR Markers</ect:classification>
		<ect:plugsInto rdf:resource="#WebcamDetails" />
		<ect:createComponent>
			<ect:ComponentDetails>
				<dc:title>ARToolkitGlyphTracker</dc:title>
				<ect:proxyComponent>true</ect:proxyComponent>
				<ect:setProperty>
					<ect:PropertyDetails>
						<dc:title>glyphFile</dc:title>
						<rdf:value>resources/markers/A.patt</rdf:value>
					</ect:PropertyDetails>
				</ect:setProperty>					
			</ect:ComponentDetails>
		</ect:createComponent>
		<ect:image>
			<ect:MediaFileLocation>
				<ect:filePath>images/armarkera.png</ect:filePath>
			</ect:MediaFileLocation>
		</ect:image>
	</ect:ARMarker>
